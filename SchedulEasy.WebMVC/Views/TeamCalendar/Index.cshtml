@model SchedulEasy.Models.TeamCalendarModel.TeamWeekForMonth
<div class="container body-content" style="background-color: white;">
    <div id="calendarTable">

        <div id="component-header">
            <div id="@Model.prevMonth"
                 class="prevMonth" style="float:left;margin-left:10px;">
                <a href="/TeamCalendar?month=@Model.prevMonth&amp;year=@Model.prevYear&amp;teamID=@Model.TeamID">
                    <span class="glyphicon glyphicon-chevron-left"></span>
                </a>
            </div>
            @Model.Week3[0].Date.ToString("MMMM") @Model.Week3[0].Date.Year
            <div id="@Model.nextMonth"
                 class="nextMonth" style="float:right; margin-right:10px;">
                <a href="/TeamCalendar?month=@Model.nextMonth&amp;year=@Model.nextYear&amp;teamID=@Model.TeamID">
                    <span class="glyphicon glyphicon-chevron-right"></span>
                </a>
            </div>
        </div>

