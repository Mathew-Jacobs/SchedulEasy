                        </th>
                        <th>
                            Wed
                        </th>
                        <th>
                            Thr
                        </th>
                        <th>
                            Fri
                        </th>
                        <th>
                            Sat
                        </th>
                    </tr>
                </thead>
                <tbody>
                    <tr class="week" id="week1">
                        @foreach (var item in Model.Week1)
                        {
                            if (item != null)
                            {
                                if (item.daycolumn == 0 || item.daycolumn == 6)
                                {
                                    <td class="weekend">
                                        <div class="busyCover" style="background-color:@item.BusyLevel"></div>
                                        <div id="@item.dateStr" class="dt">
                                            @item.dtDay
                                            <ul class="Descriptions">
                                                @foreach (var desc in item.BusyDayID)
                                                {
                                                    <li class="descList">@Html.ActionLink(desc.UserName, "Details", "BusyDay", new { id = desc.ID, teamID = Model.TeamID }, null)</li>
                                                }
                                            </ul>
                                        </div>
                                    </td>
                                }
                                else if (item.Date != DateTime.Today)
                                {
                                    <td>
                                        <div class="busyCover" style="background-color:@item.BusyLevel"></div>
                                        <div id="@item.dateStr" class="dt">
                                            @item.dtDay
                                            <ul class="Descriptions">
                                                @foreach (var desc in item.BusyDayID)
                                                {
                                                    <li class="descList">@Html.ActionLink(desc.UserName, "Details", "BusyDay", new { id = desc.ID, teamID = Model.TeamID }, null)</li>
                                                }
                                            </ul>
                                        </div>
                                    </td>
                                }
                                else
                                {
                                    <td class="selected">
                                        <div class="busyCover" style="background-color:@item.BusyLevel"></div>
                                        <div id="@item.dateStr" class="dt">
                                            @item.dtDay
                                            <ul class="Descriptions">
                                                @foreach (var desc in item.BusyDayID)
                                                {
                                                    <li class="descList">@Html.ActionLink(desc.UserName, "Details", "BusyDay", new { id = desc.ID, teamID = Model.TeamID }, null)</li>
                                                }
                                            </ul>
                                        </div>
                                    </td>
                                }
                            }
                            else
                            {
                                <td style="background-color:transparent"></td>
                            }
                        }
